	<CivilopediaPageQueries>
		<Row SectionId="CIVILIZATIONS" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT CivilizationType as PageId, null as PageGroupId, "Civilization" as PageLayoutId, Name, null as Tooltip FROM Civilizations where StartingCivilizationLevelType = "CIVILIZATION_LEVEL_FULL_CIV"</SQL>
		</Row>
		<Row SectionId="LEADERS" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT distinct Leaders.LeaderType AS PageId, null AS PageGroupId, "Leader" AS PageLayoutId, Leaders.Name, null AS Tooltip FROM Leaders INNER JOIN CivilizationLeaders on CivilizationLeaders.LeaderType = Leaders.LeaderType INNER JOIN Civilizations ON CivilizationLeaders.CivilizationType = Civilizations.CivilizationType WHERE Civilizations.StartingCivilizationLevelType = "CIVILIZATION_LEVEL_FULL_CIV"</SQL>
		</Row>
		<Row SectionId="CITYSTATES" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT CivilizationType as PageId, TypeProperties.Value as PageGroupId, "CityState" as PageLayoutId, Civilizations.Name as Name, null as Tooltip FROM Civilizations inner join TypeProperties on Civilizations.CivilizationType = TypeProperties.Type where StartingCivilizationLevelType = "CIVILIZATION_LEVEL_CITY_STATE" and TypeProperties.Name = "CityStateCategory"</SQL>
		</Row>
		<Row SectionId="DISTRICTS" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT DistrictType as PageId, null as PageGroupId, "District" as PageLayoutId, Name, null as Tooltip FROM Districts where InternalOnly = 0</SQL>
		</Row>
		<Row SectionId="BUILDINGS" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT BuildingType as PageId, PreReqDistrict as PageGroupId, "Building" as PageLayoutId, Name, null as Tooltip FROM Buildings Where IsWonder = 0 AND InternalOnly = 0</SQL>
		</Row>
		<Row SectionId="WONDERS" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT BuildingType as PageId, "Wonders" as PageGroupId, "Building" as PageLayoutId, Name, null as Tooltip FROM Buildings Where IsWonder = 1 AND InternalOnly = 0</SQL>
		</Row>
		<Row SectionId="WONDERS" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT ProjectType as PageId, "Projects" as PageGroupId, "Project" as PageLayoutId, ShortName as Name, null as Tooltip FROM Projects</SQL>
		</Row>
		<Row SectionId="UNITS" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT UnitType as PageId, FormationClass as PageGroupId, "Unit" as PageLayoutId, Name, null as Tooltip FROM Units where TraitType is null or TraitType &lt;&gt; "TRAIT_BARBARIAN"</SQL>
		</Row>
		<Row SectionId="UNITPROMOTIONS" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT UnitPromotionType as PageId, PromotionClass as PageGroupId, "UnitPromotion" as PageLayoutId, Name, null as Tooltip FROM UnitPromotions</SQL>
		</Row>
		<Row SectionId="GREATPEOPLE" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT GreatPersonIndividualType as PageId, GreatPersonClassType as PageGroupId, "GreatPerson" as PageLayoutId, Name, null as Tooltip FROM GreatPersonIndividuals</SQL>
		</Row>
		<Row SectionId="TECHNOLOGIES" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT TechnologyType as PageId, EraType as PageGroupId, "Technology" as PageLayoutId, Name, null as Tooltip FROM Technologies</SQL>
		</Row>
		<Row SectionId="CIVICS" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT CivicType as PageId, EraType as PageGroupId, "Civic" as PageLayoutId, Name, null as Tooltip FROM Civics</SQL>
		</Row>
		<Row SectionId="GOVERNMENTS" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT GovernmentType as PageId, "Government" as PageGroupId, "Government" as PageLayoutId, Name, null as Tooltip FROM Governments</SQL>
		</Row>
		<Row SectionId="GOVERNMENTS" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT PolicyType as PageId, GovernmentSlotType as PageGroupId, "Policy" as PageLayoutId, Name, null as Tooltip FROM Policies</SQL>
		</Row>
		<Row SectionId="RELIGIONS" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT ReligionType as PageId, "Religion" as PageGroupId, "Religion" as PageLayoutId, Name, null as Tooltip FROM Religions WHERE Pantheon = 0 and RequiresCustomName = 0</SQL>
		</Row>
		<Row SectionId="RELIGIONS" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT BeliefType as PageId, BeliefClassType as PageGroupId, "Belief" as PageLayoutId, Name, null as Tooltip FROM Beliefs</SQL>
		</Row>
		<Row SectionId="FEATURES" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT TerrainType as PageId, "Terrain" as PageGroupId, "Terrain" as PageLayoutId, Name, null as Tooltip FROM Terrains where Mountain = 0 and TerrainType &lt;&gt; "TERRAIN_OUTOFPLAY"</SQL>
		</Row>
		<Row SectionId="FEATURES" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT FeatureType as PageId, CASE WHEN NaturalWonder=0 THEN "Feature" ELSE "Wonder" END as PageGroupId, "Feature" as PageLayoutId, Name, null as Tooltip FROM Features</SQL>
		</Row>
		<Row SectionId="RESOURCES" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT ResourceType as PageId, ResourceClassType as PageGroupId, "Resource" as PageLayoutId, Name, null as Tooltip FROM Resources</SQL>
		</Row>
		<Row SectionId="IMPROVEMENTS" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT ImprovementType as PageId, "Improvements" as PageGroupId, "Improvement" as PageLayoutId, Name, null as Tooltip FROM Improvements Where BarbarianCamp = 0 and Goody = 0</SQL>
		</Row>
		<Row SectionId="IMPROVEMENTS" PageGroupIdColumn="PageGroupId" TooltipColumn="Tooltip" SortIndex="10">
			<SQL>SELECT RouteType as PageId, "Routes" as PageGroupId, "Route" as PageLayoutId, Name, null as Tooltip FROM Routes</SQL>
		</Row>
	</CivilopediaPageQueries>

